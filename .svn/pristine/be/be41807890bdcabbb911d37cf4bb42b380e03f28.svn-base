package entities;

/**
 * Layer Supertype base class for entity objects. Id field is borId for all
 * subclasses except XYPoint, where it is pejlingsId
 *
 * @author Richard Haley III
 */
public abstract class LayerSupertype
{

    protected long id;

    public LayerSupertype()
    {
    }

    public LayerSupertype(long id)
    {
        this.id = id;
    }
}
